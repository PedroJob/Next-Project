generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model invoice {
  id          Int      @id @default(autoincrement())
  customer_id String   @db.VarChar(255)
  amount      Float    @db.Float
  date        DateTime @default(now())
  status      String   @db.VarChar(255)
}
